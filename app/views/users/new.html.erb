<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.label :name%><br>
  <%= f.text_field :name%><br>
  <%= f.label :email%><br>
  <%= f.text_field :email%><br>
  <%= f.label :address%><br>
  <%= f.text_field :address%><br>
  <%= f.label :province%><br>
  <%= select_tag "province", options_from_collection_for_select(Province.all, "id", "name") %><br>

  <%= f.submit %>
<%end%>